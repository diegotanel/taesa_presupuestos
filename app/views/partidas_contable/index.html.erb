<h1><%=t("activerecord.models.partida_contable") %></h1>

<table>
  <tr>

    <th>Núm.</th>

    <th><%=t("activerecord.attributes.created_at") %></th>

    <th><%=t("activerecord.attributes.partida_contable.fecha_de_vencimiento") %></th>

    <th><%=t("activerecord.attributes.partida_contable.empresa") %></th>

    <th><%=t("activerecord.attributes.partida_contable.banco") %></th>

    <th><%=t("activerecord.attributes.partida_contable.solicitante") %></th>

    <th><%=t("activerecord.attributes.partida_contable.canal_de_solicitud") %></th>

    <th><%=t("activerecord.attributes.partida_contable.rubro") %></th>

    <th>Importe</th>

    <th>Tipo de moneda</th>

    <th>Cotización dólar</th>

    <th><%=t("activerecord.attributes.partida_contable.tipo_de_movimiento") %></th>

    <th><%=t("activerecord.attributes.partida_contable.cliente_proveedor") %></th>

    <th><%=t("activerecord.attributes.partida_contable.detalle") %></th>

    <th><%=t("activerecord.attributes.partida_contable.producto_trabajo") %></th>

    <th><%=t("activerecord.attributes.partida_contable.estado") %></th>

    <th><%=t("activerecord.attributes.partida_contable.motivo_de_baja_presupuestaria") %></th>

    <th><%=t("activerecord.attributes.deleted_at") %></th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

  <%= content_tag_for(:tr, @partidas_contable) do |partida_contable| %>

    <td><%= partida_contable.id %></td>

    <td><%= I18n.l(partida_contable.created_at) %></td>

    <td><%= I18n.l(partida_contable.fecha_de_vencimiento) %></td>

    <td><%= partida_contable.empresa.detalle %></td>

    <td><%= partida_contable.banco.detalle %></td>

    <td><%= partida_contable.solicitante.detalle %></td>

    <td><%= partida_contable.canal_de_solicitud.detalle %></td>

    <td><%= partida_contable.rubro.detalle %></td>

    <td><%= partida_contable.importe %></td>

    <td><%= partida_contable.importe_currency %></td>

    <td><%= partida_contable.valor_dolar %></td>

    <td><%= PartidaContable::TIPODEMOVIMIENTO.key(partida_contable.tipo_de_movimiento) %></td>

    <td><%= partida_contable.cliente_proveedor.detalle %></td>

    <td><%= partida_contable.detalle %></td>

    <td><%= partida_contable.producto_trabajo.detalle %></td>

    <td><%= PartidaContable::ESTADOS.key(partida_contable.estado) %></td>

    <td><%= partida_contable.motivo_de_baja_presupuestaria %></td>

    <td><%= partida_contable.deleted_at %></td>

    <td><%= link_to t("scaffoldi18n.show"), partida_contable %></td>
    <td><%= link_to t("scaffoldi18n.edit"), edit_partida_contable_path(partida_contable) %></td>
    <td><%= link_to t("scaffoldi18n.destroy"), partida_contable, confirm: t("scaffoldi18n.are_you_sure?"), method: :delete %></td>
    <td><%= link_to "Cancelación", partida_contable_cancelaciones_path(partida_contable) %></td>
  <% end %>
</table>

<br />

<%= link_to t('scaffoldi18n.new'), new_partida_contable_path %>


